{
  "name": "@analyticsrequired/trainer",
  "version": "0.0.0",
  "description": "Anayltics Required Training Service",
  "main": "dist/index.js",
  "author": "Kylee Tilley <kylee@testingrequired.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "prestart:cold": "npm run build",
    "start:cold": "node dist/index.js",
    "start:dev": "nodemon dist/index.js --exec \"npm run start\" --watch src",
    "test": "jest --runInBand",
    "build": "rollup -c",
    "prepublish": "npm run test && npm run build",
    "pack-preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "knex": "knex",
    "knex:latest": "knex migrate:latest",
    "knex:reset": "rm dev.sqlite3 && npm run knex:latest"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.0",
    "babel-jest": "^24.0.0",
    "jest": "^24.0.0",
    "nodemon": "^1.18.10",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "sqlite3": "^4.0.6"
  },
  "dependencies": {
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "morgan": "^1.9.1"
  }
}
